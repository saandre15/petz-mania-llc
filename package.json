{
  "name": "petz_mania_website",
  "version": "1.0.0",
  "description": "Petz Mania Website",
  "main": "./dist/index.js",
  "scripts": {
    "build-prod": "webpack --config ./webpack.prod.js",
    "build-dev": "webpack --config ./webpack.dev.js",
    "watch": "webpack serve --watch --config ./webpack.dev.js",
    "start-dev": "node -r dotenv/config dist/server.js",
    "start-prod": "pm2 start ecosystem.config.js --env production",
    "start": "NODE_ENV=development npm run start-prod",
    "stop": "pm2 stop ecosystem.config.js --env production",
    "restart": "pm2 restart ecosystem.config.js --env production",
    "reload": "pm2 reload ecosystem.config.js --env production",
    "test": "mocha --reporter spec --compilers ts:ts-node/register 'src/test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saandre15/Petz_Mania_Website.git"
  },
  "keywords": [
